
<h1>編集画面</h1>
<%= form_for(@user) do |f| %>
    <h4>名前</h4>
    <%= f.text_field :name %>
    <h4>名前（カナ）</h4>
    <%= f.text_field :kana %>
    <h4>郵便番号</h4>
    <%= f.text_field :post %>
    <h4>住所</h4>
    <%= f.text_field :address %>
    <h4>電話番号</h4>
    <%= f.text_field :tel %>
    <h4>メールアドレス</h4>
    <%= f.text_field :email %>
    <h4>パスワード</h4>
    <%= f.text_field :password %>
    <%= f.submit '保存' %>
<% end %>